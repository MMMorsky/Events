<div class="jumbotron">
  <h2>Sign in</h2>
  <p></p>

  <%= form_tag session_path do %>
      <p>Username</p> <%= text_field_tag :username, params[:username] %>
      <p></p>
      <p>Password
      <p> <%= password_field_tag :password, params[:password] %>
      <p></p><%= submit_tag "Log in", class: "btn btn-primary" %>
  <% end %>
</div>